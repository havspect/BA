    "def transform_rows_to_cols(row):\n",
    "    '''\n",
    "    Umschreiben der mode_n - rows in seperate cols und erkennen ob oev inkludiert ist.\n",
    "    '''\n",
    "    for i in range(1,7):\n",
    "        mode = row[f'mode_{i}']\n",
    "        if isinstance(mode, str):\n",
    "            if not math.isnan(row.loc[f'{mode}_length']):\n",
    "                row.loc[mode] = True\n",
    "                if mode in oev_list:\n",
    "                    row.loc['oev'] = True\n",
    "                row.loc[f'{mode}_length'] += row[f'mode_length_{i}']\n",
    "            else:\n",
    "                row.loc[mode] = True\n",
    "                if mode in oev_list:\n",
    "                    row.loc['oev'] = True\n",
    "                row.loc[f'{mode}_length'] = row[f'mode_length_{i}']\n",
    "    return row\n",
    "\n",
